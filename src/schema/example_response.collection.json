{
	"version": "3.1",
	"components": {
		"schemas": {
			"SearchRetrieveResponse": {
				"type": "object",
				"properties": {
					"searchRetrieveResponse": {
						"$ref": "#/components/schemas/SearchRetrieveResponseClass"
					}
				},
				"required": ["searchRetrieveResponse"]
			},
			"SearchRetrieveResponseClass": {
				"type": "object",
				"properties": {
					"version": {
						"type": "number"
					},
					"numberOfRecords": {
						"type": "number"
					},
					"nextRecordPosition": {
						"type": "number"
					},
					"records": {
						"$ref": "#/components/schemas/Records"
					}
				},
				"required": [
					"version",
					"numberOfRecords",
					"nextRecordPosition",
					"records"
				]
			},
			"Records": {
				"type": "object",
				"properties": {
					"record": {
						"type": "array",
						"items": {
							"$ref": "#/components/schemas/Record"
						}
					}
				},
				"required": ["record"]
			},
			"Record": {
				"type": "object",
				"properties": {
					"recordSchema": {
						"type": "string"
					},
					"recordPacking": {
						"type": "string"
					},
					"recordData": {
						"$ref": "#/components/schemas/RecordData"
					},
					"recordPosition": {
						"type": "number"
					}
				},
				"required": [
					"recordSchema",
					"recordPacking",
					"recordData",
					"recordPosition"
				]
			},
			"RecordData": {
				"type": "object",
				"properties": {
					"dc": {
						"$ref": "#/components/schemas/Dc"
					}
				},
				"required": ["dc"]
			},
			"Dc": {
				"type": "object",
				"properties": {
					"title": {
						"type": "string"
					},
					"subject": {
						"type": "array",
						"items": {
							"type": "string"
						}
					},
					"language": {
						"type": "string"
					},
					"attr_schemaLocation": {
						"type": "string"
					},
					"creator": {
						"type": "string"
					}
				},
				"required": ["title", "language", "attr_schemaLocation"]
			}
		}
	},
	"schemas": [
		{
			"$ref": "#/components/schemas/SearchRetrieveResponse"
		}
	]
}
